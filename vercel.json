{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/css/(.*)",
      "dest": "/static/css/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable",
        "Content-Type": "text/css"
      }
    },
    {
      "src": "/static/js/(.*)",
      "dest": "/static/js/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable",
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/static/images/(.*)",
      "dest": "/static/images/$1",
      "headers": {
        "Cache-Control": "public, max-age=2592000, immutable"
      }
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/((?!static).*)",
      "dest": "/api/index.py"
    }
  ]
}